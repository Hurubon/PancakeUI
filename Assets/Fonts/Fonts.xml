        <FontHeight>
            <AbsValue val='12' />
        </FontHeight>
        <Color r='1' g='1' b='1' />
    </Font>
    <Font
        name='PnkUIFontBoldItalic'
        font='Interface\AddOns\PancakeUI\Assets\Fonts\Arimo\BoldItalic.ttf'
        virtual='true'
    >
        <FontHeight>
            <AbsValue val='12' />
