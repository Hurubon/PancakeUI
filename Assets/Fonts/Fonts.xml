    xsi:schemaLocation = 'http://www.blizzard.com/wow/ui/..\FrameXML\UI.xsd'
>
    <Font
        name='PnkUIFontNormal'
        font='Interface\AddOns\PancakeUI\Assets\Fonts\Arimo\Regular.ttf'
